# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Global configuration:
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
indent_style = tab

# Markdown:
[*.md]
trim_trailing_whitespace = false
indent_style = space
indent_size = 4
